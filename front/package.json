{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:scss\" \"npm run dev:firebase\"",
    "dev:next": "next dev",
    "dev:firebase": "cd /home/app/firebase && firebase emulators:start",
    "dev:scss": "typed-scss-modules src --watch",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint -d ./ --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "typegen:scss": "typed-scss-modules src"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "firebase": "^9.16.0",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "sass": "^1.57.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^2.8.3",
    "sass-loader": "^13.2.0",
    "typed-scss-modules": "^7.0.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
